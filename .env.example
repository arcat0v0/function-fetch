# Base configuration for function-fetch
PORT=8787

# Proxy targets (array with 1 item means no load balancing)
FETCH_TARGETS=["https://a.com","https://b.com/api"]

# Health probe config (optional)
FETCH_HEALTH_PATH=/healthz
FETCH_HEALTH_TIMEOUT_MS=1500

# Retry policy (optional)
FETCH_RETRY_ON_5XX=true

# Cache adapter selection for "fastest target" (optional)
# - auto => memory,kv (kv only if FASTEST_KV is bound in wrangler config)
# - examples: kv / memory / memory,kv / none
FETCH_CACHE_ADAPTER=auto

# Cache key/ttl (used by memory + kv adapters)
FETCH_CACHE_KEY=proxy:fastest
FETCH_CACHE_TTL_SECONDS=300

# NOTE: FASTEST_KV is a Cloudflare KV binding (configure via wrangler.jsonc),
# not an environment variable.
FETCH_CACHE_KV_BINDING=
ESA_KV_NAMESPACE=